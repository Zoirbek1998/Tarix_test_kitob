<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/logo_backgraund"
    tools:context=".QuizActivity.LoginUpActivity">

    <ImageView
        android:id="@+id/backBtn"
        android:layout_width="@dimen/size_50dp"
        android:layout_height="@dimen/size_50dp"
        android:layout_margin="@dimen/size_30dp"
        android:padding="@dimen/size_10dp"
        android:src="@drawable/ic_baseline_arrow_back_24"
        app:layout_constraintEnd_toStartOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="@dimen/size_150dp"
        android:layout_height="0dp"
        android:layout_marginStart="131dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="131dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/add_logo2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="512dp"
        android:fontFamily="@font/abril_fatface"
        android:text="Exam Practice"
        android:textColor="@color/black"

        android:textSize="@dimen/text_24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

    <LinearLayout
        android:layout_width="411dp"
        android:layout_height="499dp"
        android:layout_marginTop="10dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6">

        <EditText
            android:id="@+id/fish_edit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="F I SH"
            android:textSize="@dimen/text_22sp"
            android:layout_marginTop="@dimen/size_60dp"
            android:layout_marginLeft="@dimen/size_20dp"
            android:layout_marginRight="@dimen/size_20dp"
            android:inputType="text"
            />
        <EditText
            android:id="@+id/email_edit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="E-Mail Id"
            android:textSize="@dimen/text_22sp"
            android:layout_marginTop="@dimen/size_10dp"
            android:layout_marginLeft="@dimen/size_20dp"
            android:layout_marginRight="@dimen/size_20dp"
            android:inputType="textEmailAddress"
            />
        <EditText
            android:id="@+id/parol_edit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Pario'l"
            android:layout_marginTop="@dimen/size_10dp"
            android:textSize="@dimen/text_22sp"
            android:layout_marginLeft="@dimen/size_20dp"
            android:layout_marginRight="@dimen/size_20dp"
            android:inputType="numberPassword"
            />
        <EditText
            android:id="@+id/tas_parol_edit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Tasdiqlangan Paro'l"
            android:layout_marginTop="@dimen/size_10dp"
            android:textSize="@dimen/text_22sp"
            android:layout_marginLeft="@dimen/size_20dp"
            android:layout_marginRight="@dimen/size_20dp"
            android:inputType="numberPassword"
            />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/sign_btn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="40dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="40dp"
            android:backgroundTint="@color/purple_600"
            android:text="Sign Up"
            android:textColor="@color/black"
            app:cornerRadius="@dimen/size_20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/forget_password" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>